from turtle import *
title('Welcome 2024')
bgcolor('violet')
pensize(5)
pencolor("red")
Ram_naam = ["Welcome 2024"] * 50
angle = 360 / 50
penup()
sety(-1)
for _ in range(51):
    left(angle)
    forward(260)
    if Ram_naam:
        write(Ram_naam.pop(), align="right", font=('Arial', 12, "bold"))
    backward(260)
penup()    
goto(-40, -20)
pendown()
write("Welcome 2024", font=("Arial", 50, "normal"), align="center")
hideturtle()
done()
